<launch>
    <node pkg="erl_geometry_ros" type="house_expo_2d_node" name="house_expo_2d_node" output="screen">
        <param name="map_file" value="$(find erl_geometry)/data/house_expo_room_1451.json" />
        <param name="path_file" value="$(find erl_geometry)/data/house_expo_room_1451.csv" />
        <param name="publish_pose" value="true" />
        <param name="pose_publish_rate" value="40.0" />
        <param name="path_feed_rate" value="20.0" />
        <param name="init_x" value="6.61" />
        <param name="init_y" value="6.46" />
        <param name="init_yaw" value="0.0" />
    </node>
    <node pkg="rviz" type="rviz" name="rviz" args="-d $(find erl_geometry_ros)/rviz/house_expo_2d.rviz"/>
</launch>
